@model AgendaSIG5.App.Frontend.Pages.Sedes.Editar1Model
@using AgendaSIG5.App.Dominio
@{
}
<h1>Actualizar Sede</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="sede.Id"></label>
        <input asp-for="sede.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="sede.FK_Id_Ciudad">Ciudad</label>
        <input asp-for="sede.FK_Id_Ciudad" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="sede.Nombre"></label>
        <input asp-for="sede.Nombre" class="form-control">
        <span asp-validation-for="sede.Nombre" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Actualizar Sede</button>
</form>
<a class="btn btn-secondary" asp-page="./Index1">Volver</a>